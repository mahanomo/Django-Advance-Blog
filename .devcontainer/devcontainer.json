{
    "name": "Django Dev Container",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "backend",
    "workspaceFolder": "/app",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "batisteo.vscode-django"
        ],
        "settings": {
          "python.analysis.autoSearchPaths": true,
          "python.analysis.useLibraryCodeForTypes": true
        }
      }
    }
  }
  